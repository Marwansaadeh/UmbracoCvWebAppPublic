    <div class="row">
        <div class="col-md-12">
           
        <div class="timeline">
            <div class="timeline-inner">
                @{ 
                    int counter = 0;
                }
                @foreach (var item in educations)
                {
                    string classname;
                    string delay ;
                    
                    
                    if (counter % 2 == 0)
                    {
                        classname = "single-main wow fadeInLeft";
                        delay = "0.4s";
                    }
                    else
                    {
                        classname = "single-main wow fadeInRight";
                        delay = "0.6s";
                    }
                    counter=counter+1;
                    <!-- Single Timeline -->
                            <div class="@classname" data-wow-delay="@delay">
                                <div class="single-timeline">
                                    <div class="single-content">
                                        <div class="date">
                                            <p>@item.Value("educationDate")</p>
                                        </div>
                                        <h2>@item.Value("educationTitle")</h2>
                                        <p>@item.Value("educationContent")<p>
                                    </div>
                                </div>
                            </div>
                            }


                                        </div>
        </div> 
        
                    </div>

    </div>
   
                </div>

</section>
